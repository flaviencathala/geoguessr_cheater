{
  "manifest_version": 2,

  "name": "Geoguessr cheater",
  "description": "Geoguessr cheater",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Come on, cheat !"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://geoguessr.com/*"],
      "js": ["jquery.js", "cheater.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "downloads",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ]
}
